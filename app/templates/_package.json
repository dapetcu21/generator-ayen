{
  "name": "<%= packageName %>",
  "version": "0.0.1",
  "description": "<%= description %>",
  "main": "index.js",
  "scripts": {
    "start": "node ./backend/index.js",
    "test": "./node_modules/gulp/bin/gulp.js test:once && if [ $CI ]; then npm run coveralls; fi",
    "coveralls": "cat ./coverage/phantomjs/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "postinstall": "bash ./postinstall.sh"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/<%= githubName %>/<%= packageName %>"
  },
  "keywords": <%= JSON.stringify(keywords) %>,
  "author": "<%= author %>",
  "bugs": {
    "url": "https://github.com/<%= githubName %>/<%= packageName %>/issues"
  },
  "homepage": "https://github.com/<%= githubName %>/<%= packageName %>",
  "devDependencies": {
  },
  "dependencies": {
    "aliasify": "^1.5.0",
    "bower": "^1.3.12",
    "browser-sync": "^1.7.2",
    "browserify": "^6.3.3",
    "browserify-istanbul": "^0.1.2",
    "deamdify": "^0.1.1",
    "debowerify": "^1.1.1",
    "errorhandler": "^1.3.0",
    "express": "^4.10.4",
    "fs.extra": "^1.2.1",
    "lodash": "^2.4.1",
    "main-bower-files": "^2.4.1",
    "minimist": "^1.1.0",
    "node-useref": "^0.3.8",
    "penthouse": "^0.2.51",
    "streamqueue": "^0.1.1",
    "templatizer": "^1.1.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.0.0",
    "when": "^3.6.3",
    "wiredep": "^2.2.0",
    "gulp": "^3.8.10",
    "gulp-autoprefixer": "^2.0.0",
    "gulp-concat": "^2.4.2",
    "gulp-jade": "^0.10.0",
    "gulp-load-plugins": "^0.7.1",
    "gulp-minify-css": "^0.3.11",
    "gulp-minify-html": "^0.1.7",
    "gulp-rename": "^1.2.0",
    "gulp-replace": "^0.5.0",
    "gulp-stylus": "^1.3.4",
    "gulp-tap": "^0.1.3",
    "gulp-uglify": "^1.0.1",
    "gulp-util": "^3.0.1"
  }
}
